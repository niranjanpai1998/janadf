{
	"name": "DelimitedTextQ7Sink",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"year": {
				"type": "string"
			},
			"month": {
				"type": "string"
			},
			"day": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": "@dataset().dy",
				"folderPath": "@concat(dataset().year,' ',dataset().month)",
				"container": "q7output"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"quoteChar": "\""
		},
		"schema": []
	}
}